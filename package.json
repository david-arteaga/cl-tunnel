{
  "name": "cl-tunnel",
  "version": "0.1.0",
  "description": "Opinionated CLI wrapper around cloudflared to manage ingress for a single tunnel",
  "module": "index.ts",
  "type": "module",
  "private": false,
  "bin": {
    "cl-tunnel": "dist/cl-tunnel"
  },
  "scripts": {
    "build": "bun build --compile index.ts --outfile dist/cl-tunnel",
    "prepack": "bun run build"
  },
  "files": [
    "dist/cl-tunnel",
    "README.md"
  ],
  "os": [
    "darwin"
  ],
  "cpu": [
    "arm64",
    "x64"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "@types/js-yaml": "^4.0.9"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "js-yaml": "^4.1.0",
    "yaml": "^2.8.1",
    "zod": "^4.1.0"
  }
}